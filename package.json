{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "Angular 1.5 starter project with unit tests, code coverage and component based design",
  "scripts": {
    "clean": "rimraf dist && rimraf docs && rimraf coverage",
    "build": "rimraf dist && webpack --bail --progress --profile",
    "build:run": "npm run build && live-server --port=3001 --open=dist/",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "coverage": "live-server --port=3001 --open=coverage/",
    "test": "karma start",
    "test:coverage": "npm run test && npm run coverage",
    "test:watch": "karma start --auto-watch --no-single-run",
    "docs": "jsdoc src/app -r -d docs",
    "docs:launch": "npm run docs && live-server --port=3002 --open=docs/",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Foxandxss/angular-webpack-workflow.git"
  },
  "author": "Daiyan Alamgir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Foxandxss/angular-webpack-workflow/issues"
  },
  "homepage": "https://github.com/Foxandxss/angular-webpack-workflow",
  "dependencies": {
    "@angular/router": "^0.2.0",
    "angular": "^1.5.0",
    "angular-sanitize": "^1.5.3",
    "angular-ui-select": "^0.12.100",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.1",
    "paper-css": "^0.1.2"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "jsdoc": "^3.4.0",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "live-server": "^0.9.2",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
